    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_ENV=development webpack-dev-server",
    "build": "rm -fr build/ && NODE_ENV=production webpack --progress --hide-modules",
    "deploy": "npm run build && aws --profile perso s3 sync --acl public-read --cache-control 'max-age=30' --delete build/ s3://dataaddict.fr/prenoms/",
